measured-component = {
  &(id: 1) => component-id
  &(measurement: 2) =>  corim.digest
  ? &(signers: 3) => [ + signer-type ]
}

signer-type = bytes

component-id = [
  name:      text
  ? version: version
]

;# import $version-scheme from rfc9393 as coswid

version = [
  val:      text 
  ? scheme: coswid.$version-scheme
]

; eventually: ";#import digest from rfcxxxx as corim"

corim.digest = [
  alg: (int / text)
  val: bytes
]
